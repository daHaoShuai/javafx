<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="750.0" style="-fx-background-color: #34495e;"
            xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.da.controller.TwoController">
    <Label layoutX="339.0" layoutY="33.0" text="制度模板" textFill="#2ecc71">
        <font>
            <Font name="System Bold" size="18.0"/>
        </font>
    </Label>
    <Button layoutX="483.0" layoutY="304.0" mnemonicParsing="false" onAction="#goHomePage"
            style="-fx-background-color: #2ecc71;" text="回首页" textFill="#ecf0f1">
        <font>
            <Font name="System Bold" size="14.0"/>
        </font>
    </Button>
    <Label layoutX="273.0" layoutY="118.0" text="公司名字" textFill="#2ecc71">
        <font>
            <Font name="System Bold" size="14.0"/>
        </font>
    </Label>
    <TextField fx:id="name" layoutX="375.0" layoutY="116.0" promptText="输入公司名字">
        <font>
            <Font name="System Bold" size="12.0"/>
        </font>
    </TextField>
    <Label layoutX="301.0" layoutY="197.0" text="时间" textFill="#2ecc71">
        <font>
            <Font name="System Bold" size="14.0"/>
        </font>
    </Label>
    <TextField fx:id="time" layoutX="375.0" layoutY="195.0" promptText="输入时间">
        <font>
            <Font name="System Bold" size="12.0"/>
        </font>
    </TextField>
    <Button layoutX="277.0" layoutY="304.0" mnemonicParsing="false" onAction="#createTemplate"
            style="-fx-background-color: #2ecc71;" text="生成制度模板" textFill="#ecf0f1">
        <font>
            <Font name="System Bold" size="14.0"/>
        </font>
    </Button>
</AnchorPane>
